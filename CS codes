#ENCRYPTION Ccaser Cipher
def encryption (text,s):
    result = ""
    for in range (Len(text)):
        char  = text[i]
        if (char.isupper()):
            result+=chr((ord(char) + s-65)% 26 + 65 )
        else :
              result +=chr((ord(char) + s- 97) % 26 +97 )
     
    return result
    
    #checking the above function
    text ="CSPractical"
    s =8
    print("Text  : "+ text)
    print("Shift :" + str(s))
    print("Cipher:"+encrypt(text,s))
    
